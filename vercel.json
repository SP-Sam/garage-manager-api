{
  "version": 2,
  "installCommand": "yarn",
  "buildCommand": "yarn build; yarn migrate:deploy; yarn generate",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
